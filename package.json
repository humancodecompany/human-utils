{
  "name": "@humancode_co/utils",
  "version": "1.0.3",
  "description": "A helpful library for javascript/typescript developers. :) Includes a lot of reusable functions to make easy you code process.",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "publish": "--access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/humancodecompany/utils.git"
  },
  "keywords": [
    "utils",
    "human code",
    "human code company"
  ],
  "author": "human code co.",
  "license": "MIT",
  "homepage": "https://github.com/humancodecompany/utils.git#readme",
  "dependencies": {
    "cpf-cnpj-validator": "^1.0.3",
    "date-fns": "^2.19.0"
  },
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "prettier": "^2.2.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.3"
  },
  "bugs": {
    "url": "https://github.com/humancodecompany/utils/issues"
  },
  "directories": {
    "lib": "lib"
  }
}
